<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView
        android:id="@+id/DB_mileHighCard"
        android:layout_width="35dp"
        android:layout_height="45dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="-10dp"
        app:cardBackgroundColor="#FFFFFF"
        app:cardCornerRadius="5dp"
        app:layout_constraintStart_toStartOf="@+id/tv_hello2"
        app:layout_constraintTop_toBottomOf="@+id/tv_hello2">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView28"
                android:layout_width="92dp"
                android:layout_height="123dp"
                android:scaleType="centerCrop"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/milehighbackground" />

            <ImageView
                android:id="@+id/imageView36"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="23dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:srcCompat="@drawable/beaverbttn" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="-4dp"
                android:fontFamily="@font/gotic"
                android:text="MILE\nHIGH"
                android:textColor="@color/white"
                android:textSize="7sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView36" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="480dp"
        android:layout_height="878dp"
        android:adjustViewBounds="true"
        android:scaleType="centerCrop"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/cloudsbackground4" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="413dp"
        android:layout_height="878dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/backgroundhello3" />

    <TextView
        android:id="@+id/tv_userName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/mayathin"
        android:text="User"
        android:textAlignment="viewStart"
        android:textColor="#FFFFFF"
        android:textSize="44sp"
        app:layout_constraintStart_toEndOf="@+id/DB_mileHighCard"
        app:layout_constraintTop_toTopOf="@+id/DB_mileHighCard" />

    <TextView
        android:id="@+id/tv_userName3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="-15dp"
        android:fontFamily="@font/gotic"
        android:text="Please scroll"
        android:textAlignment="center"
        android:textColor="#AE313F"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/DB_bttn_arrow"
        app:layout_constraintEnd_toEndOf="@+id/DB_bttn_arrow"
        app:layout_constraintStart_toStartOf="@+id/DB_bttn_arrow" />

    <TextView
        android:id="@+id/tv_hello2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginTop="300dp"
        android:fontFamily="@font/mayathin"
        android:text="Hello"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="100sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/iv_beaverLogo2"
        android:layout_width="93dp"
        android:layout_height="72dp"
        android:layout_marginStart="-45dp"
        android:layout_marginTop="-10dp"
        android:alpha="0.9"
        android:elevation="5dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toEndOf="@+id/tv_hello2"
        app:layout_constraintTop_toTopOf="@+id/tv_hello2"
        app:srcCompat="@drawable/logonobackground" />

    <ImageView
        android:id="@+id/DB_bttn_arrow"
        android:layout_width="50dp"
        android:layout_height="59dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_round_keyboard_arrow_down_24"
        app:tint="#AE313F" />

</androidx.constraintlayout.widget.ConstraintLayout>