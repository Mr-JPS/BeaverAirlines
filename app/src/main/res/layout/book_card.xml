<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/iv_plane2"
        android:layout_width="565dp"
        android:layout_height="496dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="55dp"
        android:adjustViewBounds="true"
        android:alpha="0.3"
        android:rotation="-12"
        app:layout_constraintBottom_toBottomOf="@+id/cardView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/home_plane" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        android:visibility="visible"
        app:cardBackgroundColor="#00FFFFFF"
        app:cardElevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/cardView"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="@+id/cardView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="381dp"
                android:layout_height="196dp"
                android:adjustViewBounds="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/homeflightsearch" />

            <AutoCompleteTextView
                android:id="@+id/dash_passengerSelect"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="11dp"
                android:fontFamily="@font/gotic"
                android:text="Select Adults"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="8sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView7"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/OB_text13"
                tools:ignore="TouchTargetSizeCheck" />

            <AutoCompleteTextView
                android:id="@+id/dash_travelSelect"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/gotic"
                android:text="Select Travel Dates"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="8sp"
                app:layout_constraintBottom_toBottomOf="@+id/dash_passengerSelect"
                app:layout_constraintStart_toEndOf="@+id/OB_text14"
                app:layout_constraintTop_toTopOf="@+id/dash_passengerSelect"
                app:layout_constraintVertical_bias="0.0"
                tools:ignore="TouchTargetSizeCheck" />

            <Button
                android:id="@+id/button3"
                android:layout_width="wrap_content"
                android:layout_height="44dp"
                android:layout_marginTop="-5dp"
                android:background="#00FFFFFF"
                android:elevation="0dp"
                android:fontFamily="@font/mayaregular"
                android:text="SEARCH NOW"
                android:textAlignment="viewEnd"
                android:textColor="#AE313F"
                android:textSize="10sp"
                app:cornerRadius="18dp"
                app:iconPadding="5dp"
                app:iconTint="#16FFFFFF"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView7"
                tools:ignore="TouchTargetSizeCheck" />

            <TextView
                android:id="@+id/OB_text13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="49dp"
                android:fontFamily="@font/gotic"
                android:text="PASSENGERS:  "
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="8sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/dash_passengerSelect"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/dash_passengerSelect" />

            <TextView
                android:id="@+id/OB_text14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="48dp"
                android:fontFamily="@font/gotic"
                android:text="TRAVEL DATES:  "
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="8sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/dash_travelSelect"
                app:layout_constraintStart_toEndOf="@+id/dash_passengerSelect"
                app:layout_constraintTop_toTopOf="@+id/dash_travelSelect" />

            <ImageButton
                android:id="@+id/imageButton"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_marginEnd="-27dp"
                android:backgroundTint="#00FFFFFF"
                android:scaleType="fitCenter"
                app:layout_constraintBottom_toBottomOf="@+id/button3"
                app:layout_constraintEnd_toStartOf="@+id/button3"
                app:layout_constraintTop_toTopOf="@+id/button3"
                app:srcCompat="@drawable/reshot_icon_airplane_r8fnbxgpkm"
                app:tint="#AE313F"
                tools:ignore="SpeakableTextPresentCheck" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cardBackgroundColor="#00FFFFFF"
        app:cardElevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/cardView2"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="379dp"
                android:layout_height="142dp"
                android:adjustViewBounds="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/homeflightcardcrop" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="60dp"
                android:layout_height="95dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/imageView2"
                app:layout_constraintTop_toTopOf="@+id/imageView2"
                app:srcCompat="@drawable/homeflightlogo" />

            <TextView
                android:id="@+id/dash_depIATA"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="22dp"
                android:fontFamily="@font/mayaregular"
                android:text="BER"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="35sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView4"
                app:layout_constraintEnd_toStartOf="@+id/imageView4"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/imageView4"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/dash_arrIATA"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="22dp"
                android:fontFamily="@font/mayaregular"
                android:text="LHR"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="35sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView4"
                app:layout_constraintEnd_toEndOf="@+id/imageView2"
                app:layout_constraintStart_toEndOf="@+id/imageView4"
                app:layout_constraintTop_toTopOf="@+id/imageView4"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/OB_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp"
                android:fontFamily="@font/gotic"
                android:text="DEPARTURE"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="8sp"
                app:layout_constraintBottom_toTopOf="@+id/dash_depIATA"
                app:layout_constraintEnd_toEndOf="@+id/dash_depIATA"
                app:layout_constraintStart_toStartOf="@+id/dash_depIATA" />

            <AutoCompleteTextView
                android:id="@+id/dash_depCitySelect"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/gotic"
                android:text="Select Location"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="8sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView4"
                app:layout_constraintEnd_toEndOf="@+id/dash_depIATA"
                app:layout_constraintStart_toStartOf="@+id/dash_depIATA"
                app:layout_constraintTop_toBottomOf="@+id/dash_depIATA"
                tools:ignore="TouchTargetSizeCheck,DuplicateSpeakableTextCheck" />

            <AutoCompleteTextView
                android:id="@+id/dash_arrCitySelect"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/gotic"
                android:text="Select Location"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="8sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView4"
                app:layout_constraintEnd_toEndOf="@+id/dash_arrIATA"
                app:layout_constraintStart_toStartOf="@+id/dash_arrIATA"
                app:layout_constraintTop_toBottomOf="@+id/dash_arrIATA"
                tools:ignore="TouchTargetSizeCheck" />

            <TextView
                android:id="@+id/OB_text4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp"
                android:fontFamily="@font/gotic"
                android:text="ARRIVAL"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="8sp"
                app:layout_constraintBottom_toTopOf="@+id/dash_arrIATA"
                app:layout_constraintEnd_toEndOf="@+id/dash_arrIATA"
                app:layout_constraintStart_toStartOf="@+id/dash_arrIATA" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>