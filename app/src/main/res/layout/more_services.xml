<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#003F51B5">

    <androidx.cardview.widget.CardView
        android:id="@+id/cv_services"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="220dp"
        app:cardBackgroundColor="#00FFFFFF"
        app:cardCornerRadius="08dp"
        app:cardElevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="320dp"
            android:layout_height="181dp"
            android:background="@drawable/moreservicescard2">

            <TextView
                android:id="@+id/tv_field5"
                android:layout_width="50dp"
                android:layout_height="34dp"
                android:fontFamily="@font/mayabold"
                android:text="Cabin Luggage"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="8sp"
                app:layout_constraintEnd_toEndOf="@+id/iv_icon5"
                app:layout_constraintStart_toStartOf="@+id/iv_icon5"
                app:layout_constraintTop_toBottomOf="@+id/iv_icon5" />

            <TextView
                android:id="@+id/tv_field6"
                android:layout_width="54dp"
                android:layout_height="34dp"
                android:fontFamily="@font/mayabold"
                android:text="Baggage Allowance "
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="8sp"
                app:layout_constraintEnd_toEndOf="@+id/iv_icon6"
                app:layout_constraintStart_toStartOf="@+id/iv_icon6"
                app:layout_constraintTop_toBottomOf="@+id/iv_icon6" />

            <TextView
                android:id="@+id/tv_field7"
                android:layout_width="50dp"
                android:layout_height="34dp"
                android:fontFamily="@font/mayabold"
                android:text="Track My Bag"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="8sp"
                app:layout_constraintEnd_toEndOf="@+id/iv_icon7"
                app:layout_constraintStart_toStartOf="@+id/iv_icon7"
                app:layout_constraintTop_toBottomOf="@+id/iv_icon7" />

            <TextView
                android:id="@+id/tv_field8"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="43dp"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/mayabold"
                android:text="More Services"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="8sp"
                app:layout_constraintBottom_toBottomOf="@+id/tv_field7"
                app:layout_constraintStart_toEndOf="@+id/iv_icon7"
                app:layout_constraintTop_toTopOf="@+id/iv_icon7"
                app:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:id="@+id/iv_icon4"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginStart="52dp"
                app:layout_constraintBottom_toBottomOf="@+id/iv_icon3"
                app:layout_constraintStart_toEndOf="@+id/iv_icon3"
                app:layout_constraintTop_toTopOf="@+id/iv_icon3"
                app:srcCompat="@drawable/reshot_icon_search_for_flight_w3rm9v4jh2"
                app:tint="#FFFFFF" />

            <TextView
                android:id="@+id/tv_field1"
                android:layout_width="50dp"
                android:layout_height="34dp"
                android:fontFamily="@font/mayabold"
                android:text="Flight Tracker"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="8sp"
                app:layout_constraintEnd_toEndOf="@+id/iv_icon1"
                app:layout_constraintStart_toStartOf="@+id/iv_icon1"
                app:layout_constraintTop_toBottomOf="@+id/iv_icon1" />

            <TextView
                android:id="@+id/tv_field2"
                android:layout_width="50dp"
                android:layout_height="34dp"
                android:fontFamily="@font/mayabold"
                android:text="Flight Schedule"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="8sp"
                app:layout_constraintEnd_toEndOf="@+id/iv_icon2"
                app:layout_constraintStart_toStartOf="@+id/iv_icon2"
                app:layout_constraintTop_toBottomOf="@+id/iv_icon2" />

            <TextView
                android:id="@+id/tv_field3"
                android:layout_width="42dp"
                android:layout_height="34dp"
                android:fontFamily="@font/mayabold"
                android:text="Covid Pass"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="8sp"
                app:layout_constraintEnd_toEndOf="@+id/iv_icon3"
                app:layout_constraintStart_toStartOf="@+id/iv_icon3"
                app:layout_constraintTop_toBottomOf="@+id/iv_icon3" />

            <TextView
                android:id="@+id/tv_field4"
                android:layout_width="50dp"
                android:layout_height="34dp"
                android:fontFamily="@font/mayabold"
                android:text="Travel History"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="8sp"
                app:layout_constraintEnd_toEndOf="@+id/iv_icon4"
                app:layout_constraintStart_toStartOf="@+id/iv_icon4"
                app:layout_constraintTop_toBottomOf="@+id/iv_icon4" />

            <ImageView
                android:id="@+id/iv_icon1"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginStart="23dp"
                android:layout_marginTop="18dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/reshot_icon_airport_46jagtwmze"
                app:tint="#FFFFFF" />

            <ImageView
                android:id="@+id/iv_icon5"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginTop="59dp"
                app:layout_constraintEnd_toEndOf="@+id/iv_icon1"
                app:layout_constraintStart_toStartOf="@+id/iv_icon1"
                app:layout_constraintTop_toBottomOf="@+id/iv_icon1"
                app:srcCompat="@drawable/reshot_icon_baggage_hk3ypux5sv"
                app:tint="#FFFFFF" />

            <ImageView
                android:id="@+id/iv_icon6"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginStart="50dp"
                app:layout_constraintBottom_toBottomOf="@+id/iv_icon5"
                app:layout_constraintStart_toEndOf="@+id/iv_icon5"
                app:layout_constraintTop_toTopOf="@+id/iv_icon5"
                app:srcCompat="@drawable/reshot_icon_baggage_8pubrqa2cv"
                app:tint="#FFFFFF" />

            <ImageView
                android:id="@+id/iv_icon7"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginStart="50dp"
                app:layout_constraintBottom_toBottomOf="@+id/iv_icon6"
                app:layout_constraintStart_toEndOf="@+id/iv_icon6"
                app:layout_constraintTop_toTopOf="@+id/iv_icon6"
                app:srcCompat="@drawable/reshot_icon_baggage_claim_kvugyz582r"
                app:tint="#FFFFFF" />

            <ImageView
                android:id="@+id/iv_icon2"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginStart="50dp"
                app:layout_constraintBottom_toBottomOf="@+id/iv_icon1"
                app:layout_constraintStart_toEndOf="@+id/iv_icon1"
                app:layout_constraintTop_toTopOf="@+id/iv_icon1"
                app:srcCompat="@drawable/reshot_icon_compass_u9wz8t6vcs"
                app:tint="#FFFFFF" />

            <ImageView
                android:id="@+id/iv_icon3"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginStart="50dp"
                app:layout_constraintBottom_toBottomOf="@+id/iv_icon2"
                app:layout_constraintStart_toEndOf="@+id/iv_icon2"
                app:layout_constraintTop_toTopOf="@+id/iv_icon2"
                app:srcCompat="@drawable/reshot_icon_passport_ztrsgwpaec"
                app:tint="#FFFFFF" />

            <ImageView
                android:id="@+id/iv_icon8"
                android:layout_width="20dp"
                android:layout_height="20dp"
                app:layout_constraintEnd_toEndOf="@+id/tv_field8"
                app:layout_constraintStart_toStartOf="@+id/tv_field8"
                app:layout_constraintTop_toBottomOf="@+id/tv_field8"
                app:srcCompat="@drawable/ic_round_arrow_forward_24" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cv_contacts"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:background="#FFFFFF"
        app:cardBackgroundColor="#24AE313F"
        app:cardCornerRadius="5dp"
        app:cardElevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/cv_services"
        app:layout_constraintStart_toStartOf="@+id/cv_services"
        app:layout_constraintTop_toBottomOf="@+id/cv_services"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#00FFFFFF">

            <ImageView
                android:id="@+id/imageView25"
                android:layout_width="28dp"
                android:layout_height="28dp"
                android:layout_marginTop="8dp"
                android:adjustViewBounds="true"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/b_logo" />

            <ImageView
                android:id="@+id/iv_icon"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginTop="4dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView25"
                app:layout_constraintEnd_toStartOf="@+id/imageView25"
                app:layout_constraintHorizontal_bias="0.29"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/imageView25"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/ic_round_mail_outline_24"
                app:tint="@color/white" />

            <ImageView
                android:id="@+id/iv_icon9"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginTop="4dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView25"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.725"
                app:layout_constraintStart_toEndOf="@+id/imageView25"
                app:layout_constraintTop_toTopOf="@+id/imageView25"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/ic_round_settings_24"
                app:tint="@color/white" />

            <TextView
                android:id="@+id/tv_field"
                android:layout_width="91dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="9dp"
                android:fontFamily="@font/mayabold"
                android:text="About Beaver Airlines"
                android:textAlignment="center"
                android:textColor="@color/red_700"
                android:textSize="8sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/imageView25"
                app:layout_constraintStart_toStartOf="@+id/imageView25"
                app:layout_constraintTop_toBottomOf="@+id/imageView25" />

            <TextView
                android:id="@+id/tv_depDateText"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/mayabold"
                android:text="Contact Us"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="8sp"
                app:layout_constraintEnd_toEndOf="@+id/iv_icon"
                app:layout_constraintStart_toStartOf="@+id/iv_icon"
                app:layout_constraintTop_toTopOf="@+id/tv_field" />

            <TextView
                android:id="@+id/tv_returnDateText"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/mayabold"
                android:text="Settings"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="8sp"
                app:layout_constraintEnd_toEndOf="@+id/iv_icon9"
                app:layout_constraintStart_toStartOf="@+id/iv_icon9"
                app:layout_constraintTop_toTopOf="@+id/tv_field" />

            <ImageView
                android:id="@+id/imageView26"
                android:layout_width="3dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/tv_field"
                app:layout_constraintEnd_toStartOf="@+id/tv_field"
                app:layout_constraintStart_toEndOf="@+id/tv_depDateText"
                app:layout_constraintTop_toTopOf="@+id/imageView25"
                app:srcCompat="@drawable/whiteline" />

            <ImageView
                android:id="@+id/iv_3"
                android:layout_width="3dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/tv_field"
                app:layout_constraintEnd_toStartOf="@+id/tv_returnDateText"
                app:layout_constraintStart_toEndOf="@+id/tv_field"
                app:layout_constraintTop_toTopOf="@+id/imageView25"
                app:layout_constraintVertical_bias="1.0"
                app:srcCompat="@drawable/whiteline" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>

