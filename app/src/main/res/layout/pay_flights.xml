<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/iv_cabinCrew"
        android:layout_width="409dp"
        android:layout_height="346dp"
        android:adjustViewBounds="true"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cabincrew"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/iv_cabinCrewCloud"
        android:layout_width="850dp"
        android:layout_height="276dp"
        android:layout_marginBottom="-180dp"
        android:adjustViewBounds="true"
        android:scaleType="centerCrop"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/iv_cabinCrew"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/iv_cabinCrew"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/clouds"
        tools:visibility="gone" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cv_payCARD"
        android:layout_width="0dp"
        android:layout_height="700dp"
        android:visibility="gone"
        app:cardBackgroundColor="#00FFFFFF"
        app:cardCornerRadius="0dp"
        app:cardElevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:visibility="visible">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/payCARDConstraint"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/iv_payFlight_backgroundBttnSheet"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="fitXY"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/flightresult_dialog_bg"
                tools:visibility="visible" />

            <ImageView
                android:id="@+id/iv_headerLogo1"
                android:layout_width="46dp"
                android:layout_height="34dp"
                android:layout_marginStart="-5dp"
                android:layout_marginTop="-10dp"
                android:visibility="gone"
                app:layout_constraintStart_toEndOf="@+id/tv_checkYourFlight"
                app:layout_constraintTop_toTopOf="@+id/tv_checkYourFlight"
                app:srcCompat="@drawable/b_logo"
                tools:visibility="visible" />

            <TextView
                android:id="@+id/tv_checkYourFlight"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:alpha="0.3"
                android:fontFamily="@font/gotic"
                android:text="CHECK"
                android:textAlignment="center"
                android:textColor="@color/purple_700"
                android:textSize="34sp"
                android:textStyle="bold"
                android:visibility="invisible"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/iv_payFlight_backgroundBttnSheet"
                tools:visibility="visible" />

            <TextView
                android:id="@+id/tv_checkYourFlight2"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="-5dp"
                android:alpha="0.3"
                android:fontFamily="@font/gotic"
                android:text="YOUR FLIGHTS"
                android:textColor="@color/purple_700"
                android:textSize="17sp"
                android:textStyle="bold"
                android:visibility="invisible"
                app:layout_constraintEnd_toEndOf="@+id/tv_checkYourFlight"
                app:layout_constraintHorizontal_bias="0.375"
                app:layout_constraintStart_toStartOf="@+id/tv_checkYourFlight"
                app:layout_constraintTop_toBottomOf="@+id/tv_checkYourFlight"
                tools:visibility="visible" />

            <TextView
                android:id="@+id/tv_departIATA"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginStart="64dp"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/mayaregular"
                android:text="BER"
                android:textColor="@color/red_700"
                android:textSize="48sp"
                android:visibility="invisible"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_checkYourFlight2"
                tools:visibility="visible" />

            <TextView
                android:id="@+id/tv_departCity"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="-8dp"
                android:fontFamily="@font/mayabold"
                android:text="CITY"
                android:textColor="@color/red_700"
                android:textSize="12sp"
                android:visibility="invisible"
                app:layout_constraintStart_toStartOf="@+id/tv_departIATA"
                app:layout_constraintTop_toBottomOf="@+id/tv_departIATA"
                tools:visibility="visible" />

            <TextView
                android:id="@+id/tv_arrivalIATA1"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="140dp"
                android:fontFamily="@font/mayaregular"
                android:text="LDN"
                android:textColor="@color/red_700"
                android:textSize="48sp"
                android:visibility="invisible"
                app:layout_constraintStart_toStartOf="@+id/tv_departIATA"
                app:layout_constraintTop_toBottomOf="@+id/tv_departIATA"
                tools:visibility="visible" />

            <TextView
                android:id="@+id/tv_arrivalCity1"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="-8dp"
                android:fontFamily="@font/mayabold"
                android:text="CITY"
                android:textColor="@color/red_700"
                android:textSize="12sp"
                android:visibility="invisible"
                app:layout_constraintStart_toStartOf="@+id/tv_arrivalIATA1"
                app:layout_constraintTop_toBottomOf="@+id/tv_arrivalIATA1"
                tools:visibility="visible" />

            <TextView
                android:id="@+id/tv_arrivalIATA2"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="138dp"
                android:fontFamily="@font/mayaregular"
                android:text="BER"
                android:textColor="@color/red_700"
                android:textSize="48sp"
                android:visibility="invisible"
                app:layout_constraintStart_toStartOf="@+id/tv_arrivalIATA1"
                app:layout_constraintTop_toBottomOf="@+id/tv_arrivalIATA1"
                tools:visibility="visible" />

            <TextView
                android:id="@+id/tv_arrivalCity2"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="-8dp"
                android:fontFamily="@font/mayabold"
                android:text="CITY"
                android:textColor="@color/red_700"
                android:textSize="12sp"
                android:visibility="invisible"
                app:layout_constraintStart_toStartOf="@+id/tv_arrivalIATA2"
                app:layout_constraintTop_toBottomOf="@+id/tv_arrivalIATA2"
                tools:visibility="visible" />

            <ImageView
                android:id="@+id/iv_dotLine1"
                android:layout_width="5dp"
                android:layout_height="215dp"
                android:layout_marginTop="10dp"
                android:visibility="invisible"
                app:layout_constraintEnd_toStartOf="@+id/tv_departIATA"
                app:layout_constraintStart_toStartOf="@+id/iv_dotPlane1"
                app:layout_constraintTop_toTopOf="@+id/tv_departIATA"
                app:srcCompat="@drawable/dottline"
                tools:visibility="visible" />

            <ImageView
                android:id="@+id/iv_dotLine2"
                android:layout_width="4dp"
                android:layout_height="228dp"
                android:layout_marginTop="6dp"
                android:visibility="invisible"
                app:layout_constraintEnd_toEndOf="@+id/iv_dotLine1"
                app:layout_constraintStart_toStartOf="@+id/iv_dotLine1"
                app:layout_constraintTop_toBottomOf="@+id/iv_dotLine1"
                app:srcCompat="@drawable/dottline"
                tools:visibility="visible" />

            <ImageView
                android:id="@+id/iv_dotPlane1"
                android:layout_width="34dp"
                android:layout_height="31dp"
                android:layout_marginStart="32dp"
                android:layout_marginTop="8dp"
