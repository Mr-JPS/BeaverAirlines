                app:tint="@color/purple_700"
                tools:visibility="visible" />

            <ImageView
                android:id="@+id/iv_dotPlane2"
                android:layout_width="34dp"
                android:layout_height="31dp"
                android:layout_marginTop="8dp"
                android:rotation="180"
                android:visibility="invisible"
                app:layout_constraintBottom_toBottomOf="@+id/cv_flight2"
                app:layout_constraintEnd_toEndOf="@+id/iv_dotPlane1"
                app:layout_constraintStart_toStartOf="@+id/iv_dotPlane1"
                app:layout_constraintTop_toTopOf="@+id/cv_flight2"
                app:srcCompat="@drawable/ic_baseline_airplanemode_active_24"
                app:tint="@color/purple_700"
                tools:visibility="visible" />

            <Button
                android:id="@+id/bttn_proceedToPasDetails"
                android:layout_width="wrap_content"
                android:layout_height="44dp"
                android:layout_marginTop="16dp"
                android:backgroundTint="@color/purple_700"
                android:fontFamily="@font/mayaregular"
                android:text="Proceed to Passenger Details"
                android:textColor="@color/white"
                android:textSize="8sp"
                android:textStyle="bold"
                android:visibility="gone"
                app:cornerRadius="20dp"
                app:iconTint="@color/white"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_arrivalCity2"
                app:strokeColor="@color/white"
                app:strokeWidth="2dp"
                tools:ignore="TouchTargetSizeCheck"
                tools:visibility="visible" />

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_flight1"
                android:layout_width="227dp"
                android:layout_height="74dp"
                android:layout_marginStart="60dp"
                android:layout_marginTop="20dp"
                android:visibility="invisible"
                app:cardCornerRadius="8dp"
                app:cardElevation="0dp"
                app:layout_constraintBottom_toTopOf="@+id/tv_arrivalIATA1"
                app:layout_constraintStart_toEndOf="@+id/iv_dotPlane1"
                app:layout_constraintTop_toBottomOf="@+id/tv_departCity"
                app:layout_constraintVertical_bias="0.0"
                tools:visibility="visible">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/iv_background1"
                        android:layout_width="232dp"
                        android:layout_height="wrap_content"
                        android:alpha="0.2"
                        android:scaleType="fitXY"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:srcCompat="@drawable/destinationbackgroundlight" />

                    <TextView
                        android:id="@+id/tv_flightNbr1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/gotic"
                        android:text="BA - 3333"
                        android:textColor="@color/purple_700"
                        android:textSize="12sp"
                        android:textStyle="bold"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/iv_background1" />

                    <TextView
                        android:id="@+id/tv_adult1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/gotic"
                        android:text="1 Adult"
                        android:textColor="@color/purple_700"
                        android:textSize="8sp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="@+id/iv_planeICON2"
                        app:layout_constraintStart_toStartOf="@+id/tv_flightNbr1"
                        app:layout_constraintTop_toTopOf="@+id/iv_planeICON2" />

                    <ImageView
                        android:id="@+id/iv_planeICON1"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_marginStart="48dp"
                        android:alpha="0.5"
                        app:layout_constraintBottom_toBottomOf="@+id/tv_flightNbr1"
                        app:layout_constraintStart_toEndOf="@+id/tv_flightNbr1"
                        app:layout_constraintTop_toTopOf="@+id/tv_flightNbr1"
                        app:layout_constraintVertical_bias="0.0"
                        app:srcCompat="@drawable/ic_round_flight_takeoff_24"
                        app:tint="@color/purple_700" />

                    <TextView
                        android:id="@+id/tv_Date1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="16dp"
                        android:fontFamily="@font/gotic"
                        android:text="2022-11-22"
                        android:textColor="@color/purple_700"
                        android:textSize="12sp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="@+id/iv_planeICON1"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/iv_planeICON1"
                        app:layout_constraintTop_toTopOf="@+id/iv_planeICON1" />

                    <TextView
                        android:id="@+id/tv_Time1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/gotic"
                        android:text="15:15"
                        android:textColor="@color/purple_700"
                        android:textSize="8sp"
                        android:textStyle="bold"
                        app:layout_constraintStart_toStartOf="@+id/tv_Date1"
                        app:layout_constraintTop_toBottomOf="@+id/tv_Date1" />

                    <ImageView
                        android:id="@+id/iv_planeICON2"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_marginTop="3dp"
                        android:alpha="0.5"
                        app:layout_constraintEnd_toEndOf="@+id/iv_planeICON1"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="@+id/iv_planeICON1"
                        app:layout_constraintTop_toBottomOf="@+id/tv_Time1"
                        app:srcCompat="@drawable/ic_round_flight_land_24"
                        app:tint="@color/purple_700" />

                    <TextView
                        android:id="@+id/tv_Date2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/gotic"
                        android:text="2022-11-22"
                        android:textColor="@color/purple_700"
                        android:textSize="12sp"
                        android:textStyle="bold"
                        app:layout_constraintStart_toStartOf="@+id/tv_Date1"
                        app:layout_constraintTop_toTopOf="@+id/iv_planeICON2" />

                    <TextView
                        android:id="@+id/tv_Time2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/gotic"
                        android:text="15:15"
                        android:textColor="@color/purple_700"
                        android:textSize="8sp"
                        android:textStyle="bold"
                        app:layout_constraintStart_toStartOf="@+id/tv_Date2"
                        app:layout_constraintTop_toBottomOf="@+id/tv_Date2" />

                    <TextView
                        android:id="@+id/tv_cabinClass1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:fontFamily="@font/gotic"
                        android:text="ECO Premium"
                        android:textColor="@color/purple_700"
                        android:textSize="8sp"
                        android:textStyle="bold"
                        app:layout_constraintStart_toStartOf="@+id/tv_adult1"
                        app:layout_constraintTop_toBottomOf="@+id/tv_adult1" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_flight2"
                android:layout_width="wrap_content"
                android:layout_height="74dp"
                android:layout_marginTop="20dp"
                android:visibility="invisible"
                app:cardCornerRadius="8dp"
                app:cardElevation="0dp"
                app:layout_constraintBottom_toTopOf="@+id/tv_arrivalIATA2"
                app:layout_constraintEnd_toEndOf="@+id/cv_flight1"
                app:layout_constraintStart_toStartOf="@+id/cv_flight1"
                app:layout_constraintTop_toBottomOf="@+id/tv_arrivalCity1"
                app:layout_constraintVertical_bias="0.0"
                tools:visibility="visible">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/iv_background2"
                        android:layout_width="232dp"
                        android:layout_height="wrap_content"
                        android:alpha="0.2"
                        android:scaleType="centerCrop"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="1.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0"
                        app:srcCompat="@drawable/destinationbackgroundlight" />

                    <TextView
                        android:id="@+id/tv_flightNbr2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/gotic"
                        android:text="BA - 3333"
                        android:textColor="@color/purple_700"
                        android:textSize="12sp"
                        android:textStyle="bold"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/iv_background2" />

                    <TextView
                        android:id="@+id/tv_adult2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/gotic"
                        android:text="1 Adult"
                        android:textColor="@color/purple_700"
                        android:textSize="8sp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="@+id/iv_planeICON4"
                        app:layout_constraintStart_toStartOf="@+id/tv_flightNbr2"
                        app:layout_constraintTop_toTopOf="@+id/iv_planeICON4" />

                    <ImageView
                        android:id="@+id/iv_planeICON3"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_marginStart="48dp"
                        android:alpha="0.5"
                        app:layout_constraintBottom_toBottomOf="@+id/tv_flightNbr2"
                        app:layout_constraintStart_toEndOf="@+id/tv_flightNbr2"
                        app:layout_constraintTop_toTopOf="@+id/tv_flightNbr2"
                        app:srcCompat="@drawable/ic_round_flight_takeoff_24"
                        app:tint="@color/purple_700" />

                    <TextView
                        android:id="@+id/tv_Date4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/gotic"
                        android:text="2022-11-22"
                        android:textColor="@color/purple_700"
                        android:textSize="12sp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="@+id/iv_planeICON4"
                        app:layout_constraintStart_toStartOf="@+id/tv_Time3"
                        app:layout_constraintTop_toTopOf="@+id/iv_planeICON4" />

                    <TextView
                        android:id="@+id/tv_Time3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/gotic"
                        android:text="15:15"
                        android:textColor="@color/purple_700"
                        android:textSize="8sp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="@+id/tv_Date3"
                        app:layout_constraintTop_toBottomOf="@+id/tv_Date3"
                        app:layout_constraintVertical_bias="0.0" />

                    <ImageView
                        android:id="@+id/iv_planeICON4"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_marginTop="3dp"
                        android:alpha="0.5"
                        app:layout_constraintEnd_toEndOf="@+id/iv_planeICON3"
                        app:layout_constraintStart_toStartOf="@+id/iv_planeICON3"
                        app:layout_constraintTop_toBottomOf="@+id/tv_Time3"
                        app:srcCompat="@drawable/ic_round_flight_land_24"
                        app:tint="@color/purple_700" />

                    <TextView
                        android:id="@+id/tv_Date3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="16dp"
                        android:fontFamily="@font/gotic"
                        android:text="2022-11-22"
                        android:textColor="@color/purple_700"
                        android:textSize="12sp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="@+id/iv_planeICON3"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/iv_planeICON3"
                        app:layout_constraintTop_toTopOf="@+id/iv_planeICON3"
                        app:layout_constraintVertical_bias="0.0" />

                    <TextView
                        android:id="@+id/tv_Time4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/gotic"
                        android:text="15:15"
                        android:textColor="@color/purple_700"
                        android:textSize="8sp"
                        android:textStyle="bold"
                        app:layout_constraintStart_toStartOf="@+id/tv_Date4"
                        app:layout_constraintTop_toBottomOf="@+id/tv_Date4" />

                    <TextView
                        android:id="@+id/tv_cabinClass2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:fontFamily="@font/gotic"
                        android:text="ECO Premium"
                        android:textColor="@color/purple_700"
                        android:textSize="8sp"
                        android:textStyle="bold"
                        app:layout_constraintStart_toStartOf="@+id/tv_adult2"
                        app:layout_constraintTop_toBottomOf="@+id/tv_adult2" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/tv_price1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/gotic"
                android:text="EUR 155.33"
                android:textColor="@color/red_700"
                android:textSize="10sp"
                android:textStyle="bold"
                android:visibility="invisible"
                app:layout_constraintEnd_toEndOf="@+id/cv_flight1"
                app:layout_constraintTop_toBottomOf="@+id/cv_flight1"
