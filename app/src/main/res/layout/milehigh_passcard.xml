<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#AFA8ACC4"
    android:orientation="vertical">

    <androidx.cardview.widget.CardView
        android:id="@+id/completeBoardingCard"
        android:layout_width="316dp"
        android:layout_height="480dp"
        android:layout_marginTop="120dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/milehighbackground"
        android:visibility="visible"
        app:cardBackgroundColor="#FFFFFF"
        app:cardCornerRadius="13dp"
        app:cardElevation="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView19"
                android:layout_width="324dp"
                android:layout_height="478dp"
                android:layout_marginTop="9dp"
                android:scaleType="centerCrop"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/milehighbackground" />

            <ImageView
                android:id="@+id/iv_MHplane"
                android:layout_width="20dp"
                android:layout_height="16dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="18dp"
                android:alpha="0.7"
                android:rotation="-45"
                android:scaleType="fitCenter"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/flugzeug2klein" />

            <TextView
                android:id="@+id/tv_MHmemberName"
                android:layout_width="242dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/gotic"
                android:text="Luca Beaverhausen"
                android:textAlignment="textStart"
                android:textColor="@color/red_700"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@+id/tv_MHstatus"
                app:layout_constraintTop_toBottomOf="@+id/tv_MHstatus" />

            <TextView
                android:id="@+id/tv_MHstatus"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="36dp"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/gotic"
                android:text="BRONZE"
                android:textAlignment="textStart"
                android:textColor="@color/purple_700"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/iv_MHlogo" />

            <TextView
                android:id="@+id/tv_MHvalidDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/gotic"
                android:text="2024 / JAN"
                android:textAlignment="textStart"
                android:textColor="@color/white"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/tv_MHvalid"
                app:layout_constraintTop_toBottomOf="@+id/tv_MHvalid" />

            <TextView
                android:id="@+id/tv_MHvalid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="104dp"
                android:fontFamily="@font/gotic"
                android:text="VALID UNTIL"
                android:textAlignment="textStart"
                android:textColor="@color/white"
                android:textSize="10sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/tv_MHmemberName"
                app:layout_constraintTop_toBottomOf="@+id/tv_MHmemberName" />

            <TextView
                android:id="@+id/tv_MHmemberNbr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:fontFamily="@font/gotic"
                android:text="MILE HIGH CLUB:   1234567890"
                android:textAlignment="textStart"
                android:textColor="@color/white"
                android:textSize="8sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/iv_MHbarcode"
                app:layout_constraintStart_toStartOf="@+id/iv_MHbarcode"
                app:layout_constraintTop_toBottomOf="@+id/iv_MHbarcode" />

            <TextView
                android:id="@+id/tv_MHpoints"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:fontFamily="@font/gotic"
                android:text="0"
                android:textAlignment="center"
                android:textColor="@color/purple_700"
                android:textSize="14sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/iv_MHplane"
                app:layout_constraintStart_toStartOf="@+id/iv_MHplane"
                app:layout_constraintTop_toBottomOf="@+id/iv_MHplane" />

            <ImageView
                android:id="@+id/iv_MHlogo"
                android:layout_width="187dp"
                android:layout_height="149dp"
                android:layout_marginStart="-25dp"
                android:layout_marginTop="24dp"
                android:adjustViewBounds="true"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/b_logo" />

            <ImageView
                android:id="@+id/iv_MHbarcode"
                android:layout_width="248dp"
                android:layout_height="37dp"
                android:layout_marginTop="20dp"
                android:adjustViewBounds="true"
                android:scaleType="centerCrop"
                app:layout_constraintEnd_toEndOf="@+id/tv_MHmemberName"
                app:layout_constraintStart_toStartOf="@+id/tv_MHvalidDate"
                app:layout_constraintTop_toBottomOf="@+id/tv_MHvalidDate"
                app:srcCompat="@drawable/whitebarcode" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>

