<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ContrainLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/OuterCard"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible">


        <ImageView
            android:id="@+id/iv_outerCard"
            android:layout_width="330dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="1dp"
            android:adjustViewBounds="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/flightresultbackground3" />

        <TextView
            android:id="@+id/tv_boeing"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gotic"
            android:text="BOEING 747-400"
            android:textAlignment="center"
            android:textColor="@color/purple_700"
            android:textSize="8sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/tv_performance"
            app:layout_constraintTop_toBottomOf="@+id/tv_performance" />

        <TextView
            android:id="@+id/tv_durationTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gotic"
            android:text="1H05Min"
            android:textAlignment="viewStart"
            android:textColor="@color/purple_700"
            android:textSize="8sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/tv_durationText"
            app:layout_constraintTop_toBottomOf="@+id/tv_durationText" />

        <TextView
            android:id="@+id/tv_beaverAlliance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gotic"
            android:text="BEAVER WORLD ALLIANCE"
            android:textAlignment="center"
            android:textColor="@color/purple_700"
            android:textSize="8sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/tv_operatedBy"
            app:layout_constraintTop_toBottomOf="@+id/tv_operatedBy" />

        <ImageView
            android:id="@+id/iv_handLgg"
            android:layout_width="20dp"
            android:layout_height="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_price"
            app:layout_constraintStart_toStartOf="@+id/iv_headerLogo"
            app:layout_constraintTop_toTopOf="@+id/tv_price"
            app:srcCompat="@drawable/reshot_icon_baggage_m7trnu93vz"
            app:tint="#FFFFFF" />

        <ImageView
            android:id="@+id/iv_lgg"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginStart="13dp"
            app:layout_constraintBottom_toBottomOf="@+id/iv_handLgg"
            app:layout_constraintStart_toEndOf="@+id/tv_handLggText"
            app:layout_constraintTop_toTopOf="@+id/iv_handLgg"
            app:srcCompat="@drawable/reshot_icon_baggage_8vh2p5afs7"
            app:tint="#FFFFFF" />

        <ImageView
            android:id="@+id/iv_fastLaneSecurity"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginTop="8dp"
            app:layout_constraintStart_toStartOf="@+id/iv_lounge"
            app:layout_constraintTop_toBottomOf="@+id/iv_lounge"
            app:srcCompat="@drawable/airport_2"
            app:tint="#FFFFFF" />

        <ImageView
            android:id="@+id/iv_prioBoarding"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginTop="8dp"
            app:layout_constraintStart_toStartOf="@+id/iv_fastLaneSecurity"
            app:layout_constraintTop_toBottomOf="@+id/iv_fastLaneSecurity"
            app:srcCompat="@drawable/gate"
            app:tint="#FFFFFF" />

        <ImageView
            android:id="@+id/iv_lounge"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="105dp"
            app:layout_constraintEnd_toEndOf="@+id/iv_outerCard"
            app:layout_constraintTop_toBottomOf="@+id/iv_barcode"
            app:srcCompat="@drawable/lounge_3"
            app:tint="#FFFFFF" />

        <ImageView
            android:id="@+id/iv_headerLogo"
            android:layout_width="90dp"
            android:layout_height="36dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="8dp"
            android:adjustViewBounds="true"
            app:layout_constraintStart_toStartOf="@+id/iv_outerCard"
            app:layout_constraintTop_toTopOf="@+id/iv_outerCard"
            app:srcCompat="@drawable/completelogo" />

        <TextView
            android:id="@+id/tv_flightNbrText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:fontFamily="@font/gotic"
            android:text="FLIGHT NUMBER"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="8sp"
            app:layout_constraintEnd_toEndOf="@+id/tv_flightNbr"
            app:layout_constraintTop_toTopOf="@+id/iv_headerLogo" />

        <TextView
            android:id="@+id/tv_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="210dp"
            android:fontFamily="@font/gotic"
            android:text="149.99"
            android:textAlignment="center"
            android:textColor="@color/red_700"
            android:textSize="17sp"
            app:layout_constraintEnd_toEndOf="@+id/tv_flightNbr"
            app:layout_constraintTop_toBottomOf="@+id/tv_flightNbr" />

        <TextView
            android:id="@+id/tv_eur"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp"
            android:fontFamily="@font/gotic"
            android:text="EUR "
            android:textAlignment="center"
            android:textColor="@color/red_700"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_price"
            app:layout_constraintEnd_toStartOf="@+id/tv_price"
            app:layout_constraintTop_toTopOf="@+id/tv_price"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/tv_loungeText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:fontFamily="@font/gotic"
            android:text="Mile-High Lounge "
            android:textAlignment="center"
            android:textColor="@color/purple_700"
            android:textSize="8sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/iv_lounge"
            app:layout_constraintStart_toEndOf="@+id/iv_lounge"
            app:layout_constraintTop_toTopOf="@+id/iv_lounge" />

        <TextView
            android:id="@+id/tv_yourExperience"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:fontFamily="@font/mayaregular"
            android:text="YOUR EXPERIENCE "
            android:textAlignment="center"
            android:textColor="@color/red_700"
            android:textSize="10sp"
            app:layout_constraintBottom_toTopOf="@+id/iv_lounge"
            app:layout_constraintStart_toStartOf="@+id/iv_lounge" />

        <TextView
            android:id="@+id/tv_theJourney"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/mayaregular"
            android:text="THE JOURNEY "
            android:textAlignment="center"
            android:textColor="@color/red_700"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_yourExperience"
            app:layout_constraintStart_toStartOf="@+id/iv_handLgg"
            app:layout_constraintTop_toTopOf="@+id/tv_yourExperience" />

        <TextView
            android:id="@+id/tv_operatedBy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="7dp"
            android:fontFamily="@font/gotic"
            android:text="Operated by"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="8sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/tv_theJourney"
            app:layout_constraintTop_toBottomOf="@+id/tv_theJourney" />

        <TextView
            android:id="@+id/tv_performance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/gotic"
            android:text="Performance"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="8sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/tv_operatedBy"
            app:layout_constraintTop_toBottomOf="@+id/tv_beaverAlliance" />

        <TextView
            android:id="@+id/tv_durationText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/gotic"
            android:text="Duration"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="8sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/tv_performance"
            app:layout_constraintTop_toBottomOf="@+id/tv_boeing" />

        <TextView
            android:id="@+id/tv_handLggText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:fontFamily="@font/gotic"
            android:text="3PC"
            android:textAlignment="center"
            android:textColor="@color/purple_700"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/iv_handLgg"
            app:layout_constraintStart_toEndOf="@+id/iv_handLgg"
            app:layout_constraintTop_toTopOf="@+id/iv_handLgg" />

        <TextView
            android:id="@+id/tv_lggText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gotic"
            android:text="55KG"
            android:textAlignment="center"
            android:textColor="@color/purple_700"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/iv_lgg"
            app:layout_constraintStart_toEndOf="@+id/iv_lgg"
            app:layout_constraintTop_toTopOf="@+id/iv_lgg" />

        <TextView
            android:id="@+id/tv_prioBoardingText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:fontFamily="@font/gotic"
            android:text="Priority Boarding"
            android:textAlignment="center"
            android:textColor="@color/purple_700"
            android:textSize="8sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/iv_prioBoarding"
            app:layout_constraintStart_toEndOf="@+id/iv_prioBoarding"
            app:layout_constraintTop_toTopOf="@+id/iv_prioBoarding" />

        <TextView
            android:id="@+id/tv_fastLaneText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:fontFamily="@font/gotic"
            android:text="Fast Lane Security"
            android:textAlignment="center"
            android:textColor="@color/purple_700"
            android:textSize="8sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/iv_fastLaneSecurity"
            app:layout_constraintStart_toEndOf="@+id/iv_fastLaneSecurity"
            app:layout_constraintTop_toTopOf="@+id/iv_fastLaneSecurity" />

        <TextView
            android:id="@+id/tv_flightNbr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="-2dp"
            android:layout_marginEnd="24dp"
            android:fontFamily="@font/mayaregular"
            android:text="BA-0649"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="@+id/iv_outerCard"
            app:layout_constraintTop_toBottomOf="@+id/tv_flightNbrText" />

        <ImageView
            android:id="@+id/iv_plane747"
            android:layout_width="417dp"
            android:layout_height="89dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="5dp"
            android:adjustViewBounds="true"
            app:layout_constraintBottom_toBottomOf="@+id/iv_outerCard"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iv_whiteLineMiddle"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/flightresultplane" />

        <Button
            android:id="@+id/bttn_letsFly"
            android:layout_width="118dp"
            android:layout_height="43dp"
            android:layout_marginTop="8dp"
            android:backgroundTint="@color/red_700"
            android:fontFamily="@font/mayaregular"
            android:text="Let's Fly"
            android:textSize="10sp"
            android:textStyle="bold"
            app:cornerRadius="20dp"
            app:icon="@drawable/ic_baseline_connecting_airports_24"
            app:iconTint="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="@+id/iv_outerCard"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iv_plane747"
            app:strokeColor="#FFFFFF"
            app:strokeWidth="1dp"
            tools:ignore="TouchTargetSizeCheck" />

        <ImageView
            android:id="@+id/iv_barcode"
            android:layout_width="99dp"
            android:layout_height="21dp"
            android:layout_marginTop="32dp"
            android:scaleType="fitCenter"
            app:layout_constraintEnd_toEndOf="@+id/iv_outerCard"
            app:layout_constraintStart_toStartOf="@+id/iv_outerCard"
            app:layout_constraintTop_toBottomOf="@+id/iv_handLgg"
            app:srcCompat="@drawable/barcode" />

        <ImageView
            android:id="@+id/iv_whiteLineMiddle"
            android:layout_width="2dp"
            android:layout_height="100dp"
            android:layout_marginTop="16dp"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toEndOf="@+id/iv_outerCard"
            app:layout_constraintStart_toStartOf="@+id/iv_outerCard"
            app:layout_constraintTop_toBottomOf="@+id/iv_barcode"
            app:srcCompat="@drawable/whiteline" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <LinearLayout
        android:id="@+id/InnerCard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <androidx.cardview.widget.CardView
                android:id="@+id/expandCard"
                android:layout_width="361dp"
                android:layout_height="wrap_content"
                android:visibility="visible"
                app:cardCornerRadius="13dp"
                app:cardElevation="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/flightresultbackground"
                    android:paddingTop="16dp"
                    android:paddingBottom="16dp">

                    <ImageView
                        android:id="@+id/imageView20"
                        android:layout_width="230dp"
                        android:layout_height="49dp"
                        android:layout_marginTop="-5dp"
                        android:adjustViewBounds="true"
                        android:visibility="visible"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.496"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:srcCompat="@drawable/flightresultplane5" />

                    <TextView
                        android:id="@+id/tv_landingTime"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/mayaregular"
                        android:text="17:10"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textSize="35sp"
                        app:layout_constraintBottom_toBottomOf="@+id/tv_takeoffTime"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/iv_window"
                        app:layout_constraintTop_toTopOf="@+id/tv_takeoffTime"
                        app:layout_constraintVertical_bias="0.0" />

                    <TextView
                        android:id="@+id/tv_arrivalText"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="3dp"
                        android:fontFamily="@font/gotic"
                        android:text="ARRIVAL"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textSize="8sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="@+id/tv_landingTime"
                        app:layout_constraintStart_toStartOf="@+id/tv_landingTime"
                        app:layout_constraintTop_toBottomOf="@+id/tv_landingTime"
                        app:layout_constraintVertical_bias="0.0" />

                    <TextView
                        android:id="@+id/tv_arrivalIATA"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="-2dp"
                        android:fontFamily="@font/gotic"
                        android:text="LHR"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="@+id/tv_arrivalText"
                        app:layout_constraintStart_toStartOf="@+id/tv_arrivalText"
                        app:layout_constraintTop_toBottomOf="@+id/tv_arrivalText" />

                    <TextView
                        android:id="@+id/tv_takeoffTime"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:fontFamily="@font/mayaregular"
                        android:text="15:05"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textSize="35sp"
                        app:layout_constraintBottom_toBottomOf="@+id/iv_innerCard"
                        app:layout_constraintEnd_toStartOf="@+id/iv_window"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/iv_window"
                        app:layout_constraintVertical_bias="0.0" />

                    <TextView
                        android:id="@+id/tv_departureText"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="3dp"
                        android:fontFamily="@font/gotic"
                        android:text="DEPARTURE"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textSize="8sp"
                        app:layout_constraintEnd_toEndOf="@+id/tv_takeoffTime"
                        app:layout_constraintStart_toStartOf="@+id/tv_takeoffTime"
                        app:layout_constraintTop_toBottomOf="@+id/tv_takeoffTime" />

                    <TextView
                        android:id="@+id/tv_departureIATA"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="-2dp"
                        android:fontFamily="@font/gotic"
                        android:text="BER"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="@+id/tv_departureText"
                        app:layout_constraintStart_toStartOf="@+id/tv_departureText"
                        app:layout_constraintTop_toBottomOf="@+id/tv_departureText" />

                    <ImageView
                        android:id="@+id/iv_window"
                        android:layout_width="74dp"
                        android:layout_height="102dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.498"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/imageView20"
                        app:layout_constraintVertical_bias="0.0"
                        app:srcCompat="@drawable/flightresultwindow1" />

                    <TextView
                        android:id="@+id/tv_classDeclar"
                        android:layout_width="44dp"
                        android:layout_height="0dp"
                        android:layout_marginTop="24dp"
                        android:fontFamily="@font/gotic"
                        android:text="FIRST CLASS"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textSize="12sp"
                        app:layout_constraintBottom_toBottomOf="@+id/iv_window"
                        app:layout_constraintEnd_toEndOf="@+id/iv_window"
                        app:layout_constraintStart_toStartOf="@+id/iv_window"
                        app:layout_constraintTop_toTopOf="@+id/iv_window"
                        app:layout_constraintVertical_bias="0.0" />

                    <TextView
                        android:id="@+id/tv_duration"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="8dp"
                        android:fontFamily="@font/gotic"
                        android:text="1H05MIN"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textSize="8sp"
                        android:visibility="gone"
                        app:layout_constraintBottom_toTopOf="@+id/bttn_price"
                        app:layout_constraintEnd_toEndOf="@+id/iv_window"
                        app:layout_constraintStart_toStartOf="@+id/iv_window" />

                    <Button
                        android:id="@+id/bttn_price"
                        android:layout_width="91dp"
                        android:layout_height="32dp"
                        android:layout_marginTop="2dp"
                        android:backgroundTint="#80AE313F"
                        android:fontFamily="@font/gotic"
                        android:text="EUR 149.99"
                        android:textSize="10dp"
                        android:visibility="gone"
                        app:layout_constraintEnd_toEndOf="@+id/iv_whiteLineFlightResult"
                        app:layout_constraintStart_toStartOf="@+id/iv_whiteLineFlightResult"
                        app:layout_constraintTop_toBottomOf="@+id/iv_whiteLineFlightResult"
                        app:strokeColor="#FFFFFF"
                        app:strokeWidth="1dp"
                        tools:ignore="TouchTargetSizeCheck" />

                    <ImageView
                        android:id="@+id/iv_whiteLineFlightResult"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="24dp"
                        android:visibility="gone"
                        app:layout_constraintEnd_toEndOf="@+id/iv_window"
                        app:layout_constraintHorizontal_bias="0.495"
                        app:layout_constraintStart_toStartOf="@+id/iv_window"
                        app:layout_constraintTop_toBottomOf="@+id/iv_window"
                        app:srcCompat="@drawable/whiteline2" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>


        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>